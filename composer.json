{
    "name": "mongi/mongicommerce",
    "description": "An elegant e-commerce package powered for the laravel framework.",
    "keywords": [
        "mongi",
        "mongicommerce"
    ],
    "homepage": "https://github.com/mongi/mongicommerce",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gianluca Mongelli",
            "email": "gianluca.mongelli@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mongi\\Mongicommerce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mongi\\Mongicommerce\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mongi\\Mongicommerce\\MongicommerceServiceProvider"
            ],
            "aliases": {
                "Mongicommerce": "Mongi\\Mongicommerce\\MongicommerceFacade"
            }
        }
    }
}
